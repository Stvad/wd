{
	"manifest_version": 2,
	"name": "Wikidata",
	"version": "0.145",
	"description": "Display information from wikidata.",
	"icons": {
		"48": "icons/wd.svg",
		"96": "icons/wd.svg"
	},	
	"page_action": {
		"browser_style": true,
		"default_icon": {
	    "19": "icons/inactive--light.svg",
	    "38": "icons/inactive--light.svg"
	  },
		"theme_icons": [{
	    "light": "icons/inactive--light.svg",
	    "dark": "icons/inactive--dark.svg",
	    "size": 16
	  }, {
	    "light": "icons/inactive-light.svg",
	    "dark": "icons/inactive--dark.svg",
	    "size": 32
	  }],
		"default_title": "Wikidata"
	},
	"sidebar_action": {
		"default_icon": "icons/wd.svg",
		"default_title": "Wikidata",
		"default_panel": "sidebar.html",
		"open_at_install": false
	},
	"permissions": [
		"activeTab",
		"storage",
		"webNavigation",
		"<all_urls>"
	],
	"background": {
		"page": "background.html"
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": [
				"content.dist.js"
			]
		}
	],
	"web_accessible_resources": [
		"sqarql-query.js"
	]
}