{
	"manifest_version": 2,
	"name": "Wikidata",
	"version": "0.145",
	"description": "Display information from wikidata.",
	"icons": {
		"48": "icons/wd.svg",
		"96": "icons/wd.svg"
	},	
	"browser_action": {
		"default_icon": "icons/inactive.svg",
		"default_title": "Wikidata"
	},
	"sidebar_action": {
		"default_icon": "icons/wd.svg",
		"default_title": "Wikidata",
		"default_panel": "sidebar/entity.html",
		"open_at_install": false
	},
	"permissions": [
		"activeTab",
		"storage",
		"webNavigation",
		"<all_urls>"
	],
	"background": {
		"page": "background.html"
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": [
				"content/resolver.js",
				"content/resolver__hash.js",
				"content/resolver__cache.js",
				"content/resolver__wikipedia.js",
				"content/resolver__commons.js",
				"content/resolver__inventaire.js",
				"content/resolver__P8966.js",
				"content/resolver__fandom.js",
				"content/resolver__wikis.js",
				"content/resolver__doi.js",
				"content/resolver__official-website.js",
				"content/content.js",
				"content/pagedata__title.js",
				"content/pagedata__description.js",
				"content/content__collect-page-links.js",
				"content/content__collect-strings.js",
				"content/content__display-metadata.js"
			]
		}
	],
	"web_accessible_resources": [
		"sqarql-query.js"
	]
}